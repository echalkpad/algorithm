package primenumber;

import java.util.ArrayList;

public class PrimeNumberTest {

	/**
	 * Prime Number: check the number of prime numbers between 101 and 200
	 * @param args
	 */
	public static void main(String[] args) {
		ArrayList<Integer> list =prime();
		System.out.println("Prime numbers between 101 and 200.");
		for(int prime: list){
			System.out.println(prime+",  ");
		}
		System.out.println("The total number is "+list.size());
	}
	
	protected static ArrayList<Integer> prime() {
		ArrayList<Integer> list = new ArrayList<Integer>();
		for(int i = 101; i < 201; i++){
			for(int j = 2; j <= Math.sqrt(i); j++){
				
			}
		}
		return list;
	}

}
