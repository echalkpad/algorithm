package primenumber;

import java.util.ArrayList;

public class PrimeNumberTest {

	/**
	 * Prime Number: check the number of prime numbers between 101 and 200
	 * @param args
	 */
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<Integer>();
		System.out.println("Prime numbers between 101 and 200.");
		for(int num = 101; num < 201; num++){
			isPrime(num);
		}
		System.out.println("The total number is "+list.size());
	}
	
	protected static ArrayList<Integer> isPrime(int num) {
		boolean flag = true;
