package multiplicationchart;

public class MultiplicationChartTest {

	/**
	 * Multiplication chart printed from the right side
	 * @param args
	 */
	public static void main(String[] args) {
		for(int i = 0; i < 9; i++){
			for(int j = 0; j < i; j++){
				for(int k = 0; k < 9-j; k++) System.out.print("   ");
			}
		}
	}

}
